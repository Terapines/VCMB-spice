{
    "name": "vcmb-spice-grammer-syntax-highlight",
    "displayName": "VCMB spice",
    "description": "VCMB spice",
    "publisher": "LostStar",
    "version": "0.0.3",
    "engines": {
        "vscode": "^1.68.1"
    },
    "categories": [
        "Programming Languages"
    ],
    "license": "MIT",
    "repository":"https://github.com/Terapines/VCMB-spice",
    "contributes": {
        "languages": [
            {
                "id": "VCMBSpice",
                "aliases": [
                    "spice",
                    "VCMBSpice"
                ],
                "extensions": [
                    ".net",
                    ".cir",
                    ".lib",
                    ".sp",
                    ".l",
                    ".sub",
                    ".scs",
                    ".inc",
                    ".cirx",
                    ".mod",
                    ".mdl",
                    ".ckt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "VCMBSpice",
                "scopeName": "source.vcmb",
                "path": "./syntaxes/VCMBSpice.tmLanguage.json"
            }
        ]
    }
}